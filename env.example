NEXT_PUBLIC_APP_URL=http://localhost:3000

# Supabase project URL, found in Settings > API
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url

# Public anon key, safe to expose to the browser
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key

# Optional: Service role key for server-side usage only (never expose to client)
# If set, server routes will use this for privileged operations.
# Keep this ONLY in .env.local or server environment, never commit actual value.
SUPABASE_SERVICE_ROLE_KEY=


